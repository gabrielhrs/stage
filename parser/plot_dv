reset
n = 3
max = 3
min = 0
width = (max-min)/n
set xrange [min:max]
set yrange [0:]
set offset graph 0.05,0.05,0.05,0.0
set xtics 1
set boxwidth width*0.9
set style fill solid 0.5
set tics out nomirror
set xlabel "dv"
set ylabel "Relative Frequency"
s = 0.562341
u = 0.587242
gauss(x) = (1/(sqrt(2*s*pi)))*exp((-(x-u)**2)/(2*s))
plot "data-dv.dat" w boxes, gauss(x)
